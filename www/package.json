{
  "name": "create-wasm-app",
  "version": "0.1.0",
  "description": "A Chip8 emulator written in Rust, Web Assembly, and JavaScript!",
  "main": "src/index.js",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "start:dev": "wasm-pack build && webpack serve -c webpack.config.js",
    "build:prod": "wasm-pack build && webpack -c webpack.prod.js",
    "esbuild": "tsc && node esbuild.dev.ts",
    "build": "wasm-pack build && tsc",
    "start2": "NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server",
    "go": "npm run build && npm run start2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rustwasm/create-wasm-app.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "author": "Patrick Harvey",
  "license": "(MIT OR Apache-2.0)",
  "bugs": {
    "url": "https://github.com/rustwasm/create-wasm-app/issues"
  },
  "homepage": "https://github.com/Patches72790/Chip8-Rust",
  "dependencies": {
    "chip8-emulator": "../pkg",
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "esbuild": "^0.17.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "copy-webpack-plugin": "^5.0.0",
    "esbuild-plugin-wasm": "^1.0.0",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hello-wasm-pack": "^0.1.0",
    "typescript": "^4.6.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  }
}
